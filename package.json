{
  "name": "angular-material-cordova-starter",
  "version": "1.0.0",
  "description": "An Angular Starter with Webpack, Cordova and Angular Material by arp0d3v",
  "keywords": [
    "angular",
    "angular6",
    "webpack",
    "typescript",
    "cordova",
    "material"
  ],
  "author": "Patrick Stapleton <patrick@tipe.io>",
  "homepage": "https://github.com/arp0d3v/angular-material-cordova-starter",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "server": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod --aot --buildOptimizer",
    "pwa": "http-server ./dist/main-app",
    "lint": "ng lint",
    "cordova:create": "cordova create cordova com.doamin.appname AppName",
    "cordova:android:key": "keytool -genkey -v -keystore MainApp.keystore -alias MainApp -keyalg RSA -keysize 2048 -validity 10000",
    "cordova:android:build": "npm run build:prod && cd cordova && cordova build android --release",
    "cordova:android": "npm run build:prod && cd cordova && cordova run android"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.10",
    "@angular/cdk": "^6.4.7",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/material": "^6.4.7",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "http-server": "^0.11.1",
    "rxjs": "~6.2.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.0",
    "@angular/cli": "~6.2.4",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.3.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2"
  }
}
